@page
@model COVID.App.FrontEnd.Pages.EditarPersonalAseoModel
@{
}
<h1>Editar Personal de Aseo</h1>
<form method="post">
    <input  type="hidden" asp-for="personalAseo.id">
       
    <div class="form-group">
        <label asp-for="personalAseo.nombre"></label>
        <input asp-for="personalAseo.nombre" class="form-control">
        <span class="text_danger" asp-validation.for="personalAseo.nombre"></span>
    </div>
    <div class="form-group">
        <label asp-for="personalAseo.apellido"></label>
        <input asp-for="personalAseo.apellido" class="form-control">
        <span class="text_danger" asp-validation.for="personalAseo.apellido"></span>
    </div>
    <div class="form-group">
        <label asp-for="personalAseo.edad"></label>
        <input asp-for="personalAseo.edad" class="form-control">
        <span class="text_danger" asp-validation.for="personalAseo.edad"></span>
    </div>
    <div class="form-group">
        <label asp-for="personalAseo.estado"></label>
        <input asp-for="personalAseo.estado" class="form-control">
        <span class="text_danger" asp-validation.for="personalAseo.estado"></span> 
    </div>
    <div class="form-group">
        <label asp-for="personalAseo.turno"></label>
        <input asp-for="personalAseo.turno" class="form-control">
        <span class="text_danger" asp-validation.for="personalAseo.turno"></span> 
    </div>
    
    <button type="submit" class="btn btn-primary">Guardar Cambios</button>
</form>